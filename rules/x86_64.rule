+(_, 1) => (1) {
}

+(_, 2) => (1) {
}

+(_, _) => (2) {
    // Move first parameter to virtual register and then
    // add the second parameter to it.
    mov %1, $1
    add %1, $2
    out %1
}

-(_, 1) => (1) {
    mov %1, $1
    dec %1
    out %1
}

-(_, 2) => (1) {
    mov %1, $1
    sub %1, 2
    out %1
}

