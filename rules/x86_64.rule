COPYTOREG(_, _) => (0) {
    // For COPYTOREG, the register name is the first parameter.
    mov $1, $3
}

COPYFROMREG(_) => (0) {
    // For COPYFROMREG, the register name is the first parameter
    mov %1, $1
    out %1
}

