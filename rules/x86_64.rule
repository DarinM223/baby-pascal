+(_, 1) => (1) {
    mov [$1, use] [%1, def]
    inc [%1, use]
    out [%1, use]
}

+(_, 2) => (1) {
    mov [$1, use], [%1, def]
    add [%1, use], [2, use], [reuse(%2, 0), def]
    out [%2, use]
}

+(_, _) => (2) {
    // Move first parameter to virtual register and then
    // add the second parameter to it.
    mov [$1, use], [%1, def]
    add [%1, use], [$2, use], [reuse(%2, 0), def]
    out [%2, use]
}

-(_, 1) => (1) {
    mov [$1, use], [%1, def]
    dec [%1, use]
    out [%1, use]
}

-(_, 2) => (1) {
    mov [$1, use] [%1, def]
    sub [%1, use], [2, use], [reuse(%2, 0), def]
    out [%2, use]
}

<(_, _) => (2) {
    terminator // Marks terminator of block.
    cmp [$1, use], [$2, use], [reuse(%1, 0), def]
    jl
}

<=(_, _) => (2) {
    terminator
    cmp [$1, use], [$2, use], [reuse(%1, 0), def]
    jle
}

Î¦(_, _) => (0) {
    phi [$1, use], [$2, use], [%1, def]
    out [%1, use]
}

:=(_) => (0) {
    out [$1, use]
}

PROJ(_, _) => (1) {
    out [proj($1, $2), use]
}

CALL(_, _, _) => (5) {
    call [$2, use], [%rax, def], [%rcx, def], [%rdx, def], [%rsi, def], [%rdi, def], [%r8, def], [%r9, def], [%r10, def], [%r11, def]
    mov [%rax, use], [%1, def]
    out [$1, use], [%1, use]
}
CALL(_, _, _, _) => (5) {
    mov [$4, use], [%rdi, def]
    call [$2, use], [%rax, def], [%rcx, def], [%rdx, def], [%rsi, def], [%rdi, def], [%r8, def], [%r9, def], [%r10, def], [%r11, def]
    mov [%rax, use], [%1, def]
    out [$1, use], [%1, use]
}
CALL(_, _, _, _, _) => (5) {
    parmov [$4, use], [$5, use], [%rdi, def], [%rsi, def]
    call [$2, use], [%rax, def], [%rcx, def], [%rdx, def], [%rsi, def], [%rdi, def], [%r8, def], [%r9, def], [%r10, def], [%r11, def]
    mov [%rax, use], [%1, def]
    out [$1, use], [%1, use]
}
CALL(_, _, _, _, _, _) => (5) {
    parmov [$4, use], [$5, use], [$6, use], [%rdi, def], [%rsi, def], [%rdx, def]
    call [$2, use], [%rax, def], [%rcx, def], [%rdx, def], [%rsi, def], [%rdi, def], [%r8, def], [%r9, def], [%r10, def], [%r11, def]
    mov [%rax, use], [%1, def]
    out [$1, use], [%1, use]
}
